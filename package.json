{
  "name": "react-json-schema-proptypes",
  "version": "0.3.0",
  "description": "Dynamically generate React propType validators based on a JSON schema",
  "publishConfig": {
    "registry": "http://registry.npm.wsjfdev.dowjones.net/"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "babelize": "babel gen -d lib --presets es2015,react,stage-0",
    "build": "tsc && npm run babelize && rimraf gen",
    "lint": "eslint ./src",
    "pretest": "npm run build && npm run lint",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive --reporter spec ./spec/**/*.js"
  },
  "keywords": [
    "React",
    "PropTypes",
    "JSON",
    "schema"
  ],
  "author": "Dow Jones",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "mocha": "^2.3.4",
    "mocha-sinon": "^1.1.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "tsd": "^0.6.5",
    "typescript": "^1.8.9"
  },
  "dependencies": {
    "ajv": "^3.8.5",
    "ajv-i18n": "^1.2.0",
    "lodash": "^4.6.1"
  }
}
